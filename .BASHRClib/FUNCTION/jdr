function jdr()  {
    echo "##############################" "$@" versus "${jnk}"/"$@"
    diff -b --recursive -U10    "$@" "${jnk}"/"$@"
#   diff -b --recursive         "$@" "${jnk}"/"$@"
#   diff -b --recursive --brief "$@" "${jnk}"/"$@"
    echo "#####################" That was "$@" versus "${jnk}"/"$@"
}

