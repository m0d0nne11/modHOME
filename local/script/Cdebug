echo ""
echo "#define MikeDEBUG"
echo "#ifdef MikeDEBUG"
echo "static char _sourceFile[] = __FILE__;"
echo "#endif                                                   /* #ifdef MikeDEBUG */"
echo ""
echo "#ifdef MikeDEBUG"
echo "printf( \"(%s:%d)\\n\", _sourceFile, __LINE__ );"
echo "fprintf( stderr, \"(%s:%d)\\n\", _sourceFile, __LINE__ );"
echo "#endif                                                   /* #ifdef MikeDEBUG */"
echo ""
exit 0
