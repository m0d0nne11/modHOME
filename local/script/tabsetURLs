#!/bin/bash
#
# Given that stdin is laid out ala the bookmarks.html generated by the
# Firefox 'Unsorted Bookmarks->ExportHTML' we extract all the URLs in
# a list as a simple text file, using argv[1] as the tabsetID.
#
sed -e '1,/<DT>.*'$1'/d' | sed -e '/<\/DL>/,$d' | eat | fmt -1 | grep -v -e '^ ICO' | tr '"' '\n' | fgrep -i http
